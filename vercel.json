{
  "functions": {
    "api/**/*.js": {
      "runtime": "@vercel/node@3.2.18"
    },
    "functions/**/*.js": {
      "runtime": "@vercel/node@3.2.18"
    }
  },
  "rewrites": [
    {
      "source": "/join/:code",
      "destination": "/api/invitation/:code"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
